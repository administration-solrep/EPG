            document="FichePresentationSD"
            doc_prefix="fps."
            emplacement="BEFORE_WHERE"
            query_part="ufnxql: SELECT fps.ecm:uuid AS id FROM FichePresentationSD AS fps"
        />
     
   </extension>
   
   <extension target="fr.dila.solonmgpp.core.service.MGPPJointureService" point="dependencies">
      <dependency prefix="n." need="fl." />
   </extension>
</component>
