<?xml version="1.0" ?>
<!-- La contribution au service Jointure pour l'application RÃ©ponses -->
<component name="fr.dila.solonmgpp.query.jointure.contrib">

  <extension target="fr.dila.solonmgpp.core.service.MGPPJointureService" point="queryassembler">
    <queryassembler class="fr.dila.solonmgpp.core.query.MGPPUFNXQLQueryAssembler" />
  </extension>
  
   <extension target="fr.dila.solonmgpp.core.service.MGPPJointureService" point="correspondences">
     <correspondence
            document="FicheLoi"
            doc_prefix="fl."
            emplacement="BEFORE_WHERE"
            query_part="ufnxql: SELECT fl.ecm:uuid AS id FROM FicheLoi AS fl"
        />
     <correspondence document="Navette" doc_prefix="n." emplacement="BEFORE_WHERE" query_part=", Navette AS n" />
     <correspondence
            document="Navette"
            doc_prefix="n."
            emplacement="AFTER_WHERE"
            query_part=" AND n.nav:idFiche = fl.ecm:uuid"
        />
     <correspondence
            document="FichePresentationOEP"
            doc_prefix="fpo."
            emplacement="BEFORE_WHERE"
            query_part="ufnxql: SELECT fpo.ecm:uuid AS id FROM FichePresentationOEP AS fpo"
        />
