<?xml version="1.0"?>

<!-- 
  Ce composant permet de contribuer des valeurs par défaut des paramètres de configuration
  de l'application SOLON MGPP.
  
  /!\ Pour configurer l'application, ne pas modifier les paramètres dans ce fichier mais
  dans nuxeo.conf ! 
 -->
<component name="fr.dila.solonmgpp.core.config">
  <extension target="fr.dila.st.core.service.ConfigService" point="configParameter">
    <!-- Configuration du bulk mode -->
    <parameter>
      <name>nuxeo.bulk.mode</name>
      <value>false</value>
    </parameter>
    
    <!-- Nom de l'utilisateur qui exécute les batch -->
    <parameter>
      <name>nuxeo.batch.user</name>
      <value>Administrator</value>
    </parameter>

  </extension>
</component>