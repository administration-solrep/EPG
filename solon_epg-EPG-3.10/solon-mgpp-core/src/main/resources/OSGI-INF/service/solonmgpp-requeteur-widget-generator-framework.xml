<?xml version="1.0"?>

<!--
  Ce composant contribue le service de génération de widget pour le requêteur de Réponses et
  Solon
-->
<component name="fr.dila.solonmgpp.core.service.MGPPRequeteurWidgetGenerator">

  <implementation class="fr.dila.solonmgpp.core.service.MGPPRequeteurWidgetGeneratorServiceImpl" />
  
  <service>
    <provide interface="fr.dila.solonmgpp.api.service.MGPPRequeteurWidgetGeneratorService"/>
  </service>
  
  <extension-point name="selecteddocuments">
    <object class="fr.dila.st.core.descriptor.SelectedDocumentDescriptor" />
  </extension-point>
  
  <extension-point name="addinfofields">
    <object class="fr.dila.st.core.descriptor.RequeteurAddInfoFieldDescriptor" />
  </extension-point>
  
  <extension-point name="widgetdescriptions">
    <object class="fr.dila.st.core.descriptor.WidgetDescriptionDescriptor" />
  </extension-point>
  
</component>
