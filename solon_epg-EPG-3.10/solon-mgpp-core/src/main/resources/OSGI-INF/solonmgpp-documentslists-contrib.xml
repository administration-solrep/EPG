<?xml version="1.0"?>

<!-- Ce composant contribue les listes de documents de l'application SOLON MGPP -->
<component name="fr.dila.solonmgpp.core.documentslists">
  
  <extension target="org.nuxeo.ecm.webapp.documentsLists.DocumentsListsService" point="list">

    <documentsList name="DEFAULT">
      <events>
        <event>folderishDocumentSelectionChanged</event>
      </events>
      <isSession>false</isSession>
      <category>CLIPBOARD</category>
      <imageURL>/icons/file_actions.gif</imageURL>
      <title>workingList.default</title>
      <defaultInCategory>true</defaultInCategory>
      <persistent>true</persistent>
    </documentsList>
    
    <documentsList name="CURRENT_SELECTION_CORBEILLE_MESSAGE">
      <events></events>
      <isSession>false</isSession>
    </documentsList>
    
    <documentsList name="CURRENT_SELECTION_CORBEILLE_DOSSIER">
      <events></events>
      <isSession>false</isSession>
    </documentsList>
    
    
  </extension>
</component>