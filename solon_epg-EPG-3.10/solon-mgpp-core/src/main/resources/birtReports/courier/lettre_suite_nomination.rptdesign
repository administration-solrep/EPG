            <method name="onFetch"><![CDATA[var digits = ["", "un", "deux", "trois", "quatre", "cinq", "six", "sept", "huit", "neuf", "dix", "onze", "douze", "treize", "quatorze", "quinze", "seize", "dix-sept", "dix-huit", "dix-neuf"];
var secDgt = ["", "dix", "vingt", "trente", "quarante", "cinquante", "soixante", "soixante", "quatre-vingt", "quatre-vingt"];
function convert(nb, rec){
	if(nb==null || nb=="")return "";
	//Enlever les zero inutiles
	nb = (parseInt(nb,10))+"";
	if(nb=="0")return rec ? "" : "zero";
	var len = nb.length;
	//only supports nbs < million
	if(len>6)return nb;
	var txt;
	if(len > 3){
		txt = convert(nb.substring(0, len-3), true);
		txt= (txt=="un") ? "" : txt+" ";
		return txt + "mille " + convert(nb.substring(len-3), true)
	}
	if(len==3){
		txt = convert(nb.substring(1), true);
		if(txt==""){
			if(nb.charAt(0) == "1")txt = "cent";
			else txt = digits[nb.charAt(len-3)] + " cents";
		}
		else txt = digits[nb.charAt(0)] + " cent " + txt;
		return txt;
	}	
	if(parseInt(nb)<20)return digits[parseInt(nb)];
	var nb1, tens = nb.charAt(len-2);
	if(tens == "1" || tens == "7" || tens == "9"){
		nb1 = parseInt("1"+nb.charAt(len-1));
	}
	else nb1 = parseInt(nb.charAt(len-1));
	txt = digits[nb1];
	//apply some rules
	if(tens == "8" &amp;&amp; nb1==0)txt = "s "+txt;
	else if(nb1==1 || (nb1==11 &amp;&amp; tens=="7")){
		if(tens=="8")txt = "-"+txt;
		else txt = " et "+txt;
	}
	else if(nb1>1)txt = "-"+txt;
	txt = secDgt[nb.charAt(len-2)] + txt;
	return txt;
}
vars["NBDEPUTES_VAR"]=convert(row["NBDEPUTES"]);
vars["NBSENATEURS_VAR"]=convert(row["NBSENATEURS"]);
vars["ARTICLEOEP1_VAR"]="__";
vars["ARTICLEOEP2_VAR"]="__";]]></method>
