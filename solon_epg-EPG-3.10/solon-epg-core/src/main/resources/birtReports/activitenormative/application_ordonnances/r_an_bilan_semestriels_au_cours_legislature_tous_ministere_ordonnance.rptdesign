            <xml-property name="queryText"><![CDATA[SELECT A.MESURE_MINISTEREPILOTE AS MINISTERERESP, 
        nvl(A.NBR_MESURES_APPELANT_DECRETS, 0) AS NBR_MESURES_APPELANT_DECRETS, 
        nvl(B.NBR_MESURES_RECU_APPLICATION, 0) AS NBR_MESURES_RECU_APPLICATION,
        (nvl(A.NBR_MESURES_APPELANT_DECRETS, 0) - nvl(B.NBR_MESURES_RECU_APPLICATION, 0)) AS NBR_MESURES_ATTENTE_DECRETS
FROM
(
SELECT nvl(V_1.MESURE_MINISTEREPILOTE,' ') as MESURE_MINISTEREPILOTE, COUNT(DISTINCT V_1.MESURE_ID) NBR_MESURES_APPELANT_DECRETS
FROM
V_AN_MESURES_APPEL_DECRET_ORDO V_1
WHERE V_1.ORDO_DATEPUBLICATION BETWEEN TO_DATE (?, 'DD/MM/YYYY HH24:MI') AND TO_DATE (?, 'DD/MM/YYYY HH24:MI')
GROUP BY V_1.MESURE_MINISTEREPILOTE
) A,
(
SELECT nvl(V_1.MESURE_MINISTEREPILOTE,' ') as MESURE_MINISTEREPILOTE, COUNT(DISTINCT V_1.MESURE_ID) NBR_MESURES_RECU_APPLICATION
FROM
V_AN_MESURES_RECU_APP_ORDO V_1
WHERE V_1.ORDO_DATEPUBLICATION BETWEEN TO_DATE (?, 'DD/MM/YYYY HH24:MI') AND TO_DATE (?, 'DD/MM/YYYY HH24:MI')
AND   V_1.DECRET_DATEPUBLICATION BETWEEN TO_DATE (?, 'DD/MM/YYYY HH24:MI') AND TO_DATE (?, 'DD/MM/YYYY HH24:MI')
GROUP BY V_1.MESURE_MINISTEREPILOTE
) B
WHERE A.MESURE_MINISTEREPILOTE = B.MESURE_MINISTEREPILOTE (+)]]></xml-property>
