                                            <expression name="expression" type="javascript">var nbrMesure = parseInt(dataSetRow["NBR_MESURES_APPELANT_DECRETS"]);&#13;
if(nbrMesure>0){&#13;
  var ratio = dataSetRow["NBR_MESURES_RECU_APPLICATION"]/ nbrMesure*100;&#13;
  if( parseInt(ratio) == parseFloat(ratio) ){&#13;
   (ratio +"%")&#13;
  } else {&#13;
    importPackage( Packages.java.text );&#13;
    var sdf = new DecimalFormat("###,##0.00");&#13;
    var roundedRatio = BirtMath.round(ratio,2)&#13;
    var myRatio = sdf.format( roundedRatio );&#13;
    var indexOfDot = myRatio.toString().indexOf(".");&#13;
    if(indexOfDot != -1){//eliminer ".00" si existe&#13;
  		if( myRatio.toString().substr((indexOfDot+1),2) == "00" ){&#13;
  			myRatio = myRatio.toString().substr(0,indexOfDot);&#13;
  		} &#13;
 	}&#13;
    (myRatio+"%")&#13;
  }&#13;
}</expression>
