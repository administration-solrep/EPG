            <xml-property name="queryText"><![CDATA[SELECT A.MESURE_MINISTEREPILOTE AS MINISTEREPILOTE, 
        nvl(A.NBR_MESURES_APPELANT_DECRETS, 0) AS NBR_MESURES_APPELANT_DECRETS, 
        nvl(B.NBR_MESURES_RECU_APPLICATION, 0) AS NBR_MESURES_RECU_APPLICATION,
        --nvl(C.NBR_MESURES_ATTENTE_DECRETS, 0) AS NBR_MESURES_ATTENTE_DECRETS
        (nvl(A.NBR_MESURES_APPELANT_DECRETS, 0) - nvl(B.NBR_MESURES_RECU_APPLICATION, 0))AS NBR_MESURES_ATTENTE_DECRETS
FROM
(
SELECT V_1.MESURE_MINISTEREPILOTE, COUNT(DISTINCT V_1.MESURE_ID) NBR_MESURES_APPELANT_DECRETS
FROM
V_AN_MESURES_APPELANT_DECRET V_1
WHERE V_1.LOI_DATEPROMULGATION BETWEEN TO_DATE (?, 'DD/MM/YYYY HH24:MI') AND TO_DATE (?, 'DD/MM/YYYY HH24:MI')
AND V_1.MESURE_MINISTEREPILOTE = ?
GROUP BY V_1.MESURE_MINISTEREPILOTE
) A,
(
SELECT V_1.MESURE_MINISTEREPILOTE, COUNT(DISTINCT V_1.MESURE_ID) NBR_MESURES_RECU_APPLICATION
FROM
V_AN_MESURES_RECU_APPLICATION V_1
WHERE V_1.LOI_DATEPROMULGATION BETWEEN TO_DATE (?, 'DD/MM/YYYY HH24:MI') AND TO_DATE (?, 'DD/MM/YYYY HH24:MI')
AND   V_1.DECRET_DATEPUBLICATION BETWEEN TO_DATE (?, 'DD/MM/YYYY HH24:MI') AND  TO_DATE (?, 'DD/MM/YYYY HH24:MI')
AND V_1.MESURE_MINISTEREPILOTE = ?
GROUP BY V_1.MESURE_MINISTEREPILOTE
) B,
(
SELECT V_1.MESURE_MINISTEREPILOTE, COUNT(DISTINCT V_1.MESURE_ID) NBR_MESURES_ATTENTE_DECRETS
FROM
V_AN_MESURES_ATTENTE_DECRET V_1
WHERE V_1.LOI_DATEPROMULGATION BETWEEN TO_DATE (?, 'DD/MM/YYYY HH24:MI') AND TO_DATE (?, 'DD/MM/YYYY HH24:MI')
AND V_1.MESURE_MINISTEREPILOTE = ?
GROUP BY V_1.MESURE_MINISTEREPILOTE
)C
WHERE A.MESURE_MINISTEREPILOTE = B.MESURE_MINISTEREPILOTE (+)
AND   A.MESURE_MINISTEREPILOTE = C.MESURE_MINISTEREPILOTE (+)]]></xml-property>
