    <method name="initialize"><![CDATA[importPackage( Packages.java.text );
var Formatter = new SimpleDateFormat("dd/MM/yyyy", reportContext.getLocale());

var dateDebut = params["DEBUTLEGISLATURE_PARAM"];
var dt = dateDebut.split("/");
if(dt[0].length==1)dt[0] = '0' + dt[0];
if(dt[1].length==1)dt[1] = '0' + dt[1];
dateDebut = dt.join("/");

vars["DEBUT_INTERVALLE1_VAR"] = dateDebut;
vars["FIN_INTERVALLE1_VAR"] = Formatter.format(new Date());
vars["DEBUT_INTERVALLE2_VAR"] = dateDebut;
vars["FIN_INTERVALLE2_VAR"] = Formatter.format(new Date());

function toddMMyyyy(madate){
   	if (madate)
   	{
   		var day = madate.getDate();
		var month = madate.getMonth()+1;
		if (day < 10) day = "0" + day ;
		if (month < 10) month = "0" + month ;
		return day + "/" + month  + "/" + madate.getFullYear();
	}
	else return " ";
}
reportContext.setPersistentGlobalVariable("toddMMyyyy", toddMMyyyy);
]]></method>
