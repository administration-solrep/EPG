    <method name="initialize"><![CDATA[importPackage( Packages.java.text );

var dateDebutLoi = params["DEBUT_INTERVALLE1_PARAM"];
var dateFinLoi = params["FIN_INTERVALLE1_PARAM"];
var dateDebutDec = params["DEBUT_INTERVALLE2_PARAM"];
var dateFinDec = params["FIN_INTERVALLE2_PARAM"];

var dt = dateDebutLoi.split("/");
if(dt[0].length==1)dt[0] = '0' + dt[0];
if(dt[1].length==1)dt[1] = '0' + dt[1];
dateDebutLoi = dt.join("/");

var dt1 = dateFinLoi.split("/");
if(dt1[0].length==1)dt1[0] = '0' + dt1[0];
if(dt1[1].length==1)dt1[1] = '0' + dt1[1];
dateFinLoi = dt1.join("/");

var dt2 = dateDebutDec.split("/");
if(dt2[0].length==1)dt2[0] = '0' + dt2[0];
if(dt2[1].length==1)dt2[1] = '0' + dt2[1];
dateDebutDec = dt2.join("/");

var dt3 = dateFinDec.split("/");
if(dt3[0].length==1)dt3[0] = '0' + dt3[0];
if(dt3[1].length==1)dt3[1] = '0' + dt3[1];
dateFinDec = dt3.join("/");

	vars["DEBUT_INTERVALLE1_VAR"] = dateDebutLoi;
	vars["FIN_INTERVALLE1_VAR"] = dateFinLoi;
	vars["DEBUT_INTERVALLE2_VAR"] = dateDebutDec;
	vars["FIN_INTERVALLE2_VAR"] = dateFinDec;//'30/06/' + new Date().getFullYear();

function toddMMyyyy(madate){
   	if (madate)
   	{
   		var day = madate.getDate();
		var month = madate.getMonth()+1;
		if (day < 10) day = "0" + day ;
		if (month < 10) month = "0" + month ;
		return day + "/" + month  + "/" + madate.getFullYear();
	}
	else return " ";
}
reportContext.setPersistentGlobalVariable("toddMMyyyy", toddMMyyyy);
]]></method>
