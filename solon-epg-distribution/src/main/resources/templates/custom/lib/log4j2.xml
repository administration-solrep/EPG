<?xml version="1.0" encoding="UTF-8" ?>
<!-- Log4j Configuration - For more configuration information and examples see https://logging.apache.org/log4j/2.x/ -->
<Configuration monitorInterval="30">
  <Appenders>
    <!-- ================================= -->
    <!-- Preserve messages in a local file -->
    <!-- ================================= -->
    <RollingFile
            name="FILE"
            fileName="${sys:nuxeo.log.dir}/server.log"
            filePattern="${sys:nuxeo.log.dir}/server-%d{yyyy-MM-dd}.log.gz"
            append="true"
        >
      <PatternLayout pattern="%d{ISO8601} %-5p [%t] [%c] %m%n" />
      <CronTriggeringPolicy schedule="0 0 0 * * ?" evaluateOnStartup="true" /> <!-- Rollover at midnight every day -->
      <DefaultRolloverStrategy />
    </RollingFile>

    <!-- ============================= -->
    <!-- Gather errors in a local file -->
    <!-- ============================= -->
    <RollingFile
            name="ERROR-FILE"
            fileName="${sys:nuxeo.log.dir}/nuxeo-error.log"
            filePattern="${sys:nuxeo.log.dir}/nuxeo-error-%d{yyyy-MM-dd}.log.gz"
            append="true"
        >
      <PatternLayout pattern="%d{ISO8601} %-5p [%t] [%c] %m%n" />
      <CronTriggeringPolicy schedule="0 0 0 * * ?" evaluateOnStartup="true" /> <!-- Rollover at midnight every day -->
      <DefaultRolloverStrategy />
    </RollingFile>

	<!-- Clone of FILE, but no stacktrace -->
    <RollingFile
            name="MESSAGES"
            fileName="${sys:nuxeo.log.dir}/messages.log"
            filePattern="${sys:nuxeo.log.dir}/messages-%d{yyyy-MM-dd}.log.gz"
            append="true"
        >
      <PatternLayout
                pattern="%d{ISO8601} %-5p (%t) %X{codeLog}%X{uidDoc}[%c]%X{userName}%X{titleDoc}%X{typeDoc}%X{infoCompl} %m%n%throwable{0}"
            />
      <CronTriggeringPolicy schedule="0 0 0 * * ?" evaluateOnStartup="true" /> <!-- Rollover at midnight every day -->
      <DefaultRolloverStrategy />
    </RollingFile>

    <!-- ============================= -->
    <!-- Gather stderr in a local file -->
    <!-- ============================= -->
    <RollingFile
            name="STDERR-FILE"
            fileName="${sys:nuxeo.log.dir}/stderr.log"
            filePattern="${sys:nuxeo.log.dir}/stderr-%d{yyyy-MM-dd}.log.gz"
            append="true"
        >
      <PatternLayout pattern="%d{ISO8601} %-5p [%t] [%c] %m%n" />
      <CronTriggeringPolicy schedule="0 0 0 * * ?" evaluateOnStartup="true" /> <!-- Rollover at midnight every day -->
      <DefaultRolloverStrategy />
    </RollingFile>

    <RollingFile
            name="REPRISE-FILE"
            fileName="${sys:nuxeo.log.dir}/reprise.log"
            filePattern="${sys:nuxeo.log.dir}/reprise-%d{yyyy-MM-dd}.log.gz"
            append="false"
        >
      <PatternLayout pattern="%d{ISO8601} %-5p [%t] [%c] %m%n" />
      <CronTriggeringPolicy schedule="0 0 0 * * ?" evaluateOnStartup="true" /> <!-- Rollover at midnight every day -->
      <DefaultRolloverStrategy />
    </RollingFile>
    <RollingFile
            name="NOTIFICATION-FILE"
            fileName="${sys:nuxeo.log.dir}/nuxeo-notification.log"
            filePattern="${sys:nuxeo.log.dir}/nuxeo-notification-%d{yyyy-MM-dd}.log.gz"
            append="true"
        >
      <PatternLayout pattern="%d{ISO8601} %-5p [%t] [%c] %m%n" />
      <CronTriggeringPolicy schedule="0 0 0 * * ?" evaluateOnStartup="true" /> <!-- Rollover at midnight every day -->
      <DefaultRolloverStrategy />
    </RollingFile>
    <RollingFile
            name="FILE-WS-CLIENT"
            fileName="${sys:nuxeo.log.dir}/ws-client.log"
            filePattern="${sys:nuxeo.log.dir}/ws-client-%d{yyyy-MM-dd}.log.gz"
