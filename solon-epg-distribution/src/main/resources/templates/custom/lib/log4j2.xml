      <PatternLayout pattern="%d{ISO8601} %-5p [%t] [%c] %m%n" />
      <CronTriggeringPolicy schedule="0 0 0 * * ?" evaluateOnStartup="true" /> <!-- Rollover at midnight every day -->
      <DefaultRolloverStrategy />
    </RollingFile>
    <RollingFile
            name="ARCHIVAGE-LOG"
            fileName="${sys:nuxeo.log.dir}/archivage.log"
            filePattern="${sys:nuxeo.log.dir}/archivage-%d{yyyy-MM-dd}.log.gz"
            append="true"
        >
      <PatternLayout pattern="%d{ISO8601} %-5p [%t] [%c] %m%n" />
      <CronTriggeringPolicy schedule="0 0 0 * * ?" evaluateOnStartup="true" /> <!-- Rollover at midnight every day -->
      <DefaultRolloverStrategy />
    </RollingFile>
    <RollingFile
            name="INDEXATION-LOG"
            fileName="${sys:nuxeo.log.dir}/indexation.log"
            filePattern="${sys:nuxeo.log.dir}/indexation-%d{yyyy-MM-dd}.log.gz"
            append="true"
        >
      <PatternLayout pattern="%d{ISO8601} %-5p [%t] [%c] %m%n" />
      <CronTriggeringPolicy schedule="0 0 0 * * ?" evaluateOnStartup="true" /> <!-- Rollover at midnight every day -->
      <DefaultRolloverStrategy />
    </RollingFile>
    <RollingFile
            name="INDEXATION-PARSE-LOG"
            fileName="${sys:nuxeo.log.dir}/indexation-parse.log"
            filePattern="${sys:nuxeo.log.dir}/indexation-parse-%d{yyyy-MM-dd}.log.gz"
            append="true"
        >
      <PatternLayout pattern="%d{ISO8601} %-5p [%t] [%c] %m%n" />
      <CronTriggeringPolicy schedule="0 0 0 * * ?" evaluateOnStartup="true" /> <!-- Rollover at midnight every day -->
      <DefaultRolloverStrategy />
    </RollingFile>
    <RollingFile
            name="INDEXATION-LARGE-LOG"
            fileName="${sys:nuxeo.log.dir}/indexation-large.log"
            filePattern="${sys:nuxeo.log.dir}/indexation-large-%d{yyyy-MM-dd}.log.gz"
            append="true"
        >
      <PatternLayout pattern="%d{ISO8601} %-5p [%t] [%c] %m%n" />
      <CronTriggeringPolicy schedule="0 0 0 * * ?" evaluateOnStartup="true" /> <!-- Rollover at midnight every day -->
      <DefaultRolloverStrategy />
    </RollingFile>
    <RollingFile
