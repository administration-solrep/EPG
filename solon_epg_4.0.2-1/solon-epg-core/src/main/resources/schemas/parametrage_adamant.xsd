<?xml version="1.0" ?>
<xs:schema
    targetNamespace="http://www.nuxeo.org/ecm/schemas/group"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:nxs="http://www.nuxeo.org/ecm/schemas/group"
>

	<!-- on inclut le schema contenant la definition de stringList -->
	<xs:include schemaLocation="base.xsd" />

	<!-- Metadonnées liées au manifest -->

	<xs:element name="numeroSolon" type="xs:int" />

	<xs:element name="originAgencyBlocIdentifier" type="xs:string" />

	<xs:element name="submisAgencyBlocIdentifier" type="xs:string" />

	<xs:element name="archivalProfile" type="xs:string" />

	<xs:element name="archivalProfileSpecific" type="xs:string" />

	<xs:element name="originatingAgencyIdentifier" type="xs:string" />

	<xs:element name="submissionAgencyIdentifier" type="xs:string" />

	<!-- Metadonnées liées à l'extraction -->

	<xs:element name="delaiEligibilite" type="xs:int" />

	<xs:element name="nbDossierExtraction" type="xs:int" />
	
	<xs:element name="vecteurPublication" type="nxs:stringList" />
	
	<xs:element name="typeActe" type="nxs:stringList" />

</xs:schema>
